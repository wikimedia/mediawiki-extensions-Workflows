{
	"root": true,
	"extends": [
		"wikimedia/client",
		"wikimedia/jquery",
		"wikimedia/mediawiki"
	],
	"globals": {
		"BpmnJS": "readonly",
		"jQuery": "readonly",
		"mediaWiki": "readonly",
		"mws": "readonly",
		"mwstake": "readonly",
		"OOJSPlus": "readonly",
		"workflows": "readonly"
	},
	"rules": {
		"es-x/no-async-functions": "off",
		"es-x/no-object-values": "off",
		"camelcase": "off",
		"max-len": "off",
		"no-jquery/no-class-state": "off",
		"no-jquery/no-extend": "off",
		"no-jquery/no-global-selector": "off",
		"no-prototype-builtins": "off",
		"no-underscore-dangle": "off",
		"no-jquery/no-done-fail": "warn"
	},
	"parserOptions": {
		"ecmaVersion": 2017
	}
}
